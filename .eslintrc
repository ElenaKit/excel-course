{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "linebreak-style": "off",
        "comma-dangle": "off",
        "quotes":"off",
        "indent":"off",
        "space-before-blocks":"off",
        "arrow-parens":"off",
        "comma-spacing":"off",
        "no-trailing-spaces":"off",
        "require-jsdoc":"off",
        "eol-last":"off"
    },
    "extends": ["eslint:recommended", "google"]
}

